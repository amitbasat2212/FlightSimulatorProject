<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.playerController">
   <center>
      <Pane prefHeight="80.0" prefWidth="789.0">
         <children>
            <Label fx:id="TimerClock" alignment="CENTER" contentDisplay="CENTER" layoutX="681.0" layoutY="12.0" prefHeight="35.0" prefWidth="109.0" text="  :    :  " textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="PlaySpeed" alignment="CENTER" layoutX="630.0" layoutY="14.0" prefHeight="32.0" prefWidth="57.0" text="1.5">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="533.0" layoutY="18.0" lineSpacing="1.0" text="Play Speed" textFill="#5472cc">
               <font>
                  <Font name="Franklin Gothic Medium" size="19.0" />
               </font>
            </Label>

            <Line endX="-85.0" startX="-877.0" stroke="#00000036" />
            <ScrollBar fx:id="ScrollFlight" blockIncrement="1.0" cache="true" value="1.0"   layoutX="1.0" layoutY="58.0" prefHeight="15.0" prefWidth="785.0" />
            <Button fx:id="play" layoutX="295.0" layoutY="17.0" mnemonicParsing="false" onAction="#play" text="play" />
            <Button fx:id="stop" layoutX="395.0" layoutY="17.0" mnemonicParsing="false" onAction="#stop" text="stop" />
            <Button fx:id="pause" layoutX="339.0" layoutY="17.0" mnemonicParsing="false" onAction="#pause" text="pause" />
            <Button fx:id="rewind" layoutX="256.0" layoutY="17.0" mnemonicParsing="false" onAction="#rewind" text="&lt;&lt;" />
            <Button fx:id="forward" layoutX="442.0" layoutY="17.0" mnemonicParsing="false" onAction="#forward" text="&gt;&gt;" />
            <Button fx:id="fastForward" layoutX="483.0" layoutY="17.0" mnemonicParsing="false" onAction="#fastForward" text="&gt;&gt;&gt;" />
            <Button fx:id="fastRewind" layoutX="208.0" layoutY="17.0" mnemonicParsing="false" onAction="#fastRewind" text="&lt;&lt;&lt;" />
            <Label fx:id="SecondsTimer"  alignment="CENTER" contentDisplay="CENTER" layoutX="713.0" layoutY="12.0" prefHeight="35.0" prefWidth="109.0" text="00" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label fx:id="MinutesTimer"  alignment="CENTER" contentDisplay="CENTER" layoutX="681.0" layoutY="12.0" prefHeight="35.0" prefWidth="109.0" text="00" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label fx:id="HoursTimer"  alignment="CENTER" contentDisplay="CENTER" layoutX="646.0" layoutY="12.0" prefHeight="35.0" prefWidth="109.0" text="00" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </center>
</BorderPane>